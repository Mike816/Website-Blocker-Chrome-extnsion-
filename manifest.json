{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "permissions": ["storage", "webNavigation", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    }
  }
  