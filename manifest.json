{
	"manifest_version": 3,
	"name": "Website Blocker",
	"version": "1.0",
	"description": "Blocks specified websites.",
	"background": {
	  "service_worker": "background.js"
	},
	"permissions": [
	  "webRequest",
	  "webRequestBlocking",
	  "unsafe-inline",
	  "<all_urls>"
	],
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": {
		
		"16": "/assets/icons/48.png",
		"128": "/assets/icons/128.png",
		"48": "/assets/icons/48.png"
		
	  },
	  "icons": {
		"16": "/assets/icons/48.png",
		"128": "/assets/icons/128.png",
		"48": "/assets/icons/48.png"
	  }
	}
  }
  